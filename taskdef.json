{
    "executionRoleArn": "arn:aws:iam::563781936732:role/ecsTaskExecutionRole",
    "containerDefinitions": [
        {
            "name": "javademo8",
            "image": "<IMAGE1_NAME>",
            "essential": true,
            "portMappings": [
                {
                    "hostPort": 0,
                    "protocol": "tcp",
                    "containerPort": 8080
                }
            ]
        }
    ],
    "requiresCompatibilities": [
        "EC2"
    ],
    "networkMode": "bridge",
    "cpu": "512",
    "memory": "512",
    "family": "javademo8"
}